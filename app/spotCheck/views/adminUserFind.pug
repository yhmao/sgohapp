extends _layout

block content 
    h1.text-center 查找用户
    .div 
        form(action="/spotCheck/admin/user/find",method="POST")
            .row
                input(type="text",name="name",id="name", placeholder="请输入用户名...(不填查全部)")
            .row
                input(type="text",name="cellphone",id="cellphone", placeholder="请输入用户手机号...(不填查全部)")
            .row
                input(type="submit",name="submit",id="submit", value="点击查找用户")


